<!DOCTYPE html>
<html>
<head>
  <title>RailsGirlsSubmissions</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= stylesheet_link_tag 'https://fonts.googleapis.com/css?family=Lato:400,300,700&subset=latin,latin-ext' %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <p id="notice"><%= notice %></p>

  <%= render 'layouts/header' %>

  <div class="l-wrapper">
    <article class="l-submissions-container">
      <%= yield %>
    </article>
  </div>
</body>
</html>
